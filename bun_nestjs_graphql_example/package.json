{
  "name": "bun_nestjs_graphql_example",
  "module": "src/main.tsx",
  "type": "module",
  "scripts": {
    "format": "prettier --write \"src\" \"assets\" package.json tsconfig.json",
    "nbuild": "bun run nest build",
    "start:dev": "bun --watch ./src/main.tsx",
    "start:debug": "nest start --debug --watch",
    "start:prod": "bun ./src/main.tsx",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "init:db": "bun run ./src/init-database.ts",
    "clean:db": "rm -f database.sqlite && bun run init:db",
    "start": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@types/bun": "latest",
    "@types/express": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "madge": "^8.0.0",
    "prettier": "^3.5.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@apollo/server": "^4.12.0",
    "@nestjs/apollo": "^13.1.0",
    "@nestjs/common": "^11.1.0",
    "@nestjs/core": "^11.1.0",
    "@nestjs/graphql": "^13.1.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/typeorm": "^11.0.0",
    "class-validator": "^0.14.2",
    "graphql": "^16.11.0",
    "pg": "^8.15.6",
    "sqlite3": "^5.1.7",
    "typeorm": "0.3.22",
    "reflect-metadata": "^0.2.2"
  }
}
